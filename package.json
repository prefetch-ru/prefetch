{
  "name": "@prefetchru/prefetch",
  "version": "1.1.0",
  "description": "Мгновенная загрузка страниц для российского веба. Instant page loading for Russian web.",
  "main": "prefetch.js",
  "module": "prefetch.esm.js",
  "browser": "dist/prefetch.min.js",
  "unpkg": "dist/prefetch.min.js",
  "jsdelivr": "dist/prefetch.min.js",
  "exports": {
    ".": {
      "import": "./prefetch.esm.js",
      "require": "./prefetch.js",
      "browser": "./dist/prefetch.min.js"
    },
    "./esm": "./prefetch.esm.js",
    "./esm/min": "./dist/prefetch.esm.min.js"
  },
  "files": [
    "src/",
    "prefetch.js",
    "prefetch.esm.js",
    "dist/prefetch.min.js",
    "dist/prefetch.esm.min.js",
    "README.md"
  ],
  "scripts": {
    "minify": "node build.js",
    "build": "npm run minify",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prefetch-ru/prefetch.git"
  },
  "keywords": [
    "prefetch",
    "preload",
    "performance",
    "speed",
    "instant",
    "russia",
    "bitrix",
    "tilda",
    "insales",
    "speculation-rules",
    "prerender"
  ],
  "author": "Sergey Makarov <prefetch.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prefetch-ru/prefetch/issues"
  },
  "homepage": "https://prefetch.ru",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "rollup": "^4.28.0",
    "terser": "^5.27.0"
  }
}
